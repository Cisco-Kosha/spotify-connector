{
  "version": "2.0.0",
  "oauth2_provider_name": "spotify",
  "fields": [
    {
      "field": "ACCESS_TOKEN",
      "displayName": "OAuth2 Access Token",
      "type": "String",
      "minLength": 50,
      "maxLength": 250,
      "required": true,
      "description": "Spotify OAuth2 access token",
      "example": "BQBczUJ3DNEHVWy6E9rf5S6m_KhGfQBryerT-PCOEkLiYrBPF5MPCIPyHgPqKpl-cZEyyJTdefN-5cHjqdPR1UBRHsfclCPIVins9igHX0wvM2pt22HFGPIh7nYd0mLXcQkfQAFpO6RepSZ0pP8xVOfsBYwr1mY6YT4JxC4xD_Rgo0EIXp63gLOEOji76eZgaX1_mK15",
      "helpSection": "Spotify OAuth2 access token"
    },
    {
      "field": "REFRESH_TOKEN",
      "displayName": "OAuth2 refresh token",
      "type": "String",
      "minLength": 10,
      "maxLength": 180,
      "required": false,
      "description": "Spotify OAuth2 refresh token",
      "example": "QB4_wcm5EqOkRLj3KADJxl_IkHDu3tc-P7WmUedhROMtVCRJsUjvfSmQqfuj1c2GvvR4ZnrlsXdHjGlu9S71a0sqonDUW9eenLvIKgfuFzX_9AXBmdUxEoDwzbZSnta0Bo",
      "helpSection": "Spotify OAuth2 refresh token"
    },
    {
      "field": "EXPIRES_AT",
      "displayName": "OAuth2 Expiry Time",
      "type": "String",
      "minLength": 5,
      "maxLength": 30,
      "required": false,
      "description": "Spotify OAuth2 token refresh time",
      "example": "2022-11-18T15:08:55.642141+05:30",
      "helpSection": "Spotify OAuth2 token refresh time"
    },
    {
      "field": "provider",
      "displayName": "OAuth2 Provider Name",
      "type": "String",
      "minLength": 1,
      "maxLength": 60,
      "required": true,
      "description": "Spotify OAuth2 provider name",
      "example": "spotify",
      "helpSection": "Spotify OAuth2 provider name"
    },
    {
      "field": "email",
      "displayName": "Email fetched from OAuth2 auth flow",
      "type": "String",
      "minLength": 1,
      "maxLength": 60,
      "required": true,
      "description": "Spotify email fetched from OAuth2 auth flow",
      "example": "bob@email.com",
      "helpSection": "Spotify email fetched from OAuth2 auth flow"
    },
    {
      "field": "AUTH_TYPE",
      "displayName": "Spotify Auth Type",
      "type": "String",
      "minLength": 4,
      "maxLength": 60,
      "required": true,
      "description": "Spotify API Auth Type",
      "default": "OAUTH2",
      "example": "OAUTH2",
      "helpSection": ""
    },
    {
      "field": "SERVER_URL",
      "displayName": "Spotify Server URL",
      "type": "String",
      "minLength": 4,
      "maxLength": 60,
      "required": true,
      "description": "Spotify API Server URL",
      "default": "https://api.spotify.com/v1",
      "example": "https://api.spotify.com/v1",
      "helpSection": "Use the default value https://api.spotify.com"
    }
  ]
}
